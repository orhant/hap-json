{
    "name": "orhant/hap-json",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "Json data model for Yii2",
    "keywords": ["hap", "yii2", "json", "entity"],
    "homepage": "https://gitlab.com/orhant/hap-json",
    "support": {
        "source": "https://gitlab.com/orhant/hap-json.git"
    },
    "authors": [
        {
            "name": "Orhan t",
            "email": "orhant@hap.org",
            "homepage": "https://hap.org",
            "role": "developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "hap\\json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hap\\tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~8.0",

        "ext-json": "*",

        "yiisoft/yii2": "~2.0",

        "hap/yii2-validate": "~3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.5"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"},
        {"type": "gitlab", "url": "https://gitlab.com/hap/yii2-validate.git"}
    ]
}
